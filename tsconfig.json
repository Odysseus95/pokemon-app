{
		"compilerOptions": {
				"target": "es6",
				"module": "commonjs",
				"moduleResolution": "node",
				"sourceMap": true,
				"emitDecoratorMetadata": true,
				"experimentalDecorators": true,
				"lib": [
						"es2015",
						"dom"
				],
				"noImplicitAny": true,
				"suppressImplicitAnyIndexErrors": true,
				"removeComments": false,
				"outDir": "dist"
		}
}


